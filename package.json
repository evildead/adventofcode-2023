{
  "name": "adventofcode-2023",
  "version": "1.1.0",
  "engines": {
    "node": ">=16"
  },
  "description": "Advent Of Code https://adventofcode.com/2023",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "node build/index.js",
    "start:dev": "nodemon src/index.ts",
    "clean": "rimraf build/",
    "build": "npm run clean && tsc && npm i --package-lock-only",
    "test": "jest --watchAll=false --coverage --coverageReporters=html",
    "test:ci": "jest --watchAll=false --coverage --ci --reporters=jest-junit --reporters=default",
    "lint-fix": "eslint ./src --fix --cache"
  },
  "repository": {
    "type": "git",
    "url": "git@<repo-url>"
  },
  "keywords": [
    "Node.js",
    "Typescript",
    "Advent-Of-Code",
    "Christmas"
  ],
  "author": "Danilo Carrabino",
  "license": "ISC",
  "jest-junit": {
    "outputDirectory": "./test-reports"
  },
  "devDependencies": {
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "figlet": "^1.7.0"
  }
}
